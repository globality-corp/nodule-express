{
    "name": "@globality/nodule-express",
    "version": "0.8.0",
    "description": "Node Express Conventions",
    "main": "lib/index.js",
    "repository": "git@github.com:globality-corp/nodule-express",
    "author": "Globality Engineering",
    "license": "MIT",
    "files": [
        "lib/"
    ],
    "scripts": {
        "clean": "rm -rf ./lib",
        "build": "babel src --out-dir lib --ignore '**/__tests__/*,**/__mocks__/*' && yarn build:types",
        "build:types": "tsc",
        "lint": "eslint src --cache",
        "test": "jest",
        "typecheck": "tsc --noEmit",
        "test:watch": "jest --watch",
        "verify": "yarn lint && yarn test",
        "preyalc": "yarn clean && yarn build"
    },
    "dependencies": {
        "basic-auth": "^1.1.0",
        "connect-requestid": "^1.1.0",
        "cors": "^2.8.4",
        "dataloader": "^1.4.0",
        "express": "^4.19.2",
        "helmet": "^3.12.0",
        "http-status-codes": "^1.3.0",
        "jsonwebtoken": "^9.0.0",
        "lodash": "^4.17.5",
        "path-to-regexp": "^6.2.0"
    },
    "peerDependencies": {
        "@globality/nodule-config": "3.0.0-dev.73",
        "@globality/nodule-logging": "2.0.0-dev.13",
        "@globality/nodule-openapi": "1.0.0-dev.36"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.0",
        "@babel/core": "^7.7.2",
        "@babel/node": "^7.7.0",
        "@babel/preset-env": "^7.7.1",
        "@globality/nodule-config": "3.0.0-dev.73",
        "@globality/nodule-logging": "2.0.0-dev.13",
        "@globality/nodule-openapi": "1.0.0-dev.36",
        "@types/node": "^18",
        "eslint": "^4.18.2",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-plugin-import": "^2.9.0",
        "jest": "^24.9.0",
        "supertest": "^3.0.0",
        "typescript": "^5.6.2"
    },
    "jest": {
        "testRegex": "/__tests__/.*test\\.js$",
        "modulePaths": [
            "<rootDir>/src"
        ]
    },
    "resolutions": {
        "mem": "^5.1.1",
        "lodash": "^4.17.15",
        "stringstream": "^1.0.0",
        "js-yaml": "^3.13.1",
        "just-extend": "^4.0.2",
        "extend": "^2.0.2",
        "randomatic": "^3.1.1",
        "cryptiles": "^4.1.3",
        "handlebars": "^4.5.1",
        "set-value": "^3.0.1"
